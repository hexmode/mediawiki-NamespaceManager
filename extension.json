{
	"name": "NamespaceManager",
	"namemsg": "nsmgr-extensionname",
	"version": "0.2",
	"author": [
		"[//hexmode.com Mark A. Hershberger]"
	],
	"url": "https://mediawiki.org/wiki/Extension:NamespaceManager",
	"descriptionmsg": "nsmgr-desc",
	"license-name": "GPL-3.0+",
	"config": {
		"_prefix": "NamespaceManager",
		"MapFile": "ns.json"
	},
	"requires": {
		"MediaWiki": ">= 1.27.0"
	},
	"type": "special",
	"MessagesDirs": {
		"NamespaceManager": [
			"i18n"
		]
	},
	"Hooks": {
		"NamespaceIsMovable": [
			"MediaWiki\\Extensions\\NamespaceManager\\Hooks::onNamespaceIsMovable"
		],
		"SearchableNamespaces": [
			"MediaWiki\\Extensions\\NamespaceManager\\Hooks::onSearchableNamespaces"
		],
		"EditPageTosSummary": [
			"MediaWiki\\Extensions\\NamespaceManager\\Hooks::onEditPageTosSummary"
		],
		"EditPageCopyrightWarning": [
			"MediaWiki\\Extensions\\NamespaceManager\\Hooks::onEditPageCopyrightWarning"
		],
		"ChangesListSpecialPageQuery": [
			"MediaWiki\\Extensions\\NamespaceManager\\Hooks::onChangesListSpecialPageQuery"
		]
	},
	"ConfigRegistry": {
		"ldapprovider": "MediaWiki\\Extensions\\NamespaceManager\\Config::newInstance"
	},
	"callback": "MediaWiki\\Extensions\\NamespaceManager\\Hooks::init",
	"load_composer_autoloader": true,
	"manifest_version": 1
}
