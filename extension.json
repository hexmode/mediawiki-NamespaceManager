{
	"name": "NamespaceManager",
	"namemsg": "nsmgr-extensionname",
	"version": "0.1",
	"author": [
		"[//hexmode.com Mark A. Hershberger]"
	],
	"descriptionmsg": "nsmgr-desc",
	"type": "special",
	"license-name": "GPL-3.0+",
	"MessagesDirs": {
		"NamespaceManager": [
			"i18n"
		]
	},
	"Hooks": {
		"NamespaceIsMovable": [
			"MediaWiki\\Extension\\NamespaceManager\\Hook::onNamespaceIsMovable"
		],
		"SearchableNamespaces": [
			"MediaWiki\\Extension\\NamespaceManager\\Hook::onSearchableNamespaces"
		],
		"EditPageTosSummary": [
			"MediaWiki\\Extension\\NamespaceManager\\Hook::onEditPageTosSummary"
		],
		"EditPageCopyrightWarning": [
			"MediaWiki\\Extension\\NamespaceManager\\Hook::onEditPageCopyrightWarning"
		],
		"ChangesListSpecialPageQuery": [
			"MediaWiki\\Extension\\NamespaceManager\\Hook::onChangesListSpecialPageQuery"
		]
	},
	"config": {
		"_prefix": "NamespaceManager",
		"MapFile": "ns.json"
	},
	"ConfigRegistry": {
		"ldapprovider": "MediaWiki\\Extension\\NamespaceManager\\Config::newInstance"
	},
	"callback": "MediaWiki\\Extension\\NamespaceManager\\Hook::init",
	"load_composer_autoloader": true,
	"manifest_version": 1
}
